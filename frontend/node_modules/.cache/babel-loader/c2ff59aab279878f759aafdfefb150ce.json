AAA,KAAA,CAAA,cAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,KAAA,GAAA,CAAA,KAAA,KAAA;AACA;AACA,KAVA;AAWA,IAAA,YAXA,0BAWA;AACA;AACA,UAAA,OAAA,GAAA,KAAA;;AACA,UAAA,KAAA,QAAA,CAAA,KAAA,CAAA,OAAA,CAAA,EACA;AACA;AACA,aAAA,cAAA,GAAA,KAAA,QAAA;AACA,OAJA,MAKA;AACA;AACA,aAAA,cAAA,GAAA,KAAA,QAAA,GAAA,GAAA,GAAA,KAAA,QAAA;AACA;;AACA,UAAA,KAAA,KAAA,IAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,KAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EAAA,KAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,KAAA,cAAA;AACA,aAAA,YAAA;AACA,OALA,MAKA;AACA;AACA,aAAA,eAAA;AACA;AAEA,KAjCA;AAkCA,IAAA,eAlCA,6BAkCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,WACA,QADA,6BAEA;AACA,kBAAA,EAAA,EAAA,KAAA,CAAA,IAAA,CAAA,EADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA;AAFA,iBAFA,CAFA;;AAAA;AAEA,gBAAA,MAFA;AAQA,gBAAA,OAAA,CAAA,GAAA,CAAA,eAAA,EAAA,MAAA,CAAA,IAAA;;AACA,gBAAA,KAAA,CAAA,sBAAA,CAAA,MAAA,CAAA,IAAA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,KAAA,CAAA,gBAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAhDA;AAiDA,IAAA,YAjDA,0BAiDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,WACA,QADA,0BAEA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,IAAA,CAAA,EADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAFA,iBAFA,CAFA;;AAAA;AAEA,gBAAA,MAFA;AAQA,gBAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,MAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,sBAAA,CAAA,MAAA,CAAA,IAAA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,aAAA,EAAA,MAAA,CAAA,gBAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KA/DA;AAgEA,IAAA,cAhEA,4BAgEA;AACA,UAAA,SAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA;AAAA,OAAA,CAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,EAAA,QAAA;AACA;AAnEA;AAvCA,CAAA","sourcesContent":["<template>\n    <div>               \n        <div>\n            <v-btn\n                icon\n                style=\"float: left;\"\n                @click=\"[changeHeart(),storedMypage()]\"  \n                >\n                <v-icon>{{ heart ? 'mdi-heart' : 'mdi-heart-outline' }}</v-icon>\n            </v-btn>\n\n            <v-btn\n                style=\"font-family: GmarketSansBold;\"\n                color=\"#FCCE67\"\n                @click=\"gotoDetailPage()\"\n                text\n                >\n            Details\n            </v-btn> \n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapState, mapMutations } from 'vuex'\nimport axios from 'axios'\nimport {BASE_URL} from '@/api.js'\nexport default {\n    props:{\n        sido_sgg:String\n     \n    },\n    mounted(){\n            //특별시일 경우 *2 해서 다시 저장\n            var pattern = /\\s/g;\n            if(this.sido_sgg.match(pattern))\n            {\n            //특별시가 아님\n            this.checkSido_sgg = this.sido_sgg\n            }\n            else{\n            //특별시\n            this.checkSido_sgg = this.sido_sgg + \" \" + this.sido_sgg\n            }\n\n            if(this.userStoredCities.includes(this.checkSido_sgg)){   \n                this.heart = true\n            }\n    },\n    computed:{\n        ...mapState([\n            'searchResults'  ,\n            'user',\n            'userStoredCities'\n        ])\n    },\n    data(){\n        return{\n            checkSido_sgg:'',\n            heart: false,\n            show: false,\n            sido_sgg_value:'',\n            storedCities:''\n            \n        }\n    },\n    methods:{\n        ...mapMutations([\n            'updateUserStoredCities'\n        ]),\n        changeHeart(){\n            if(this.user.email == undefined){\n                alert('로그인 후 사용해주세요')\n            }else{\n                this.heart = !this.heart\n            }      \n        },\n        storedMypage(){    \n                //특별시일 경우 *2 해서 다시 저장\n                var pattern = /\\s/g;\n                if(this.sido_sgg.match(pattern))\n                {\n                //특별시가 아님\n                this.sido_sgg_value = this.sido_sgg\n                }\n                else{\n                //특별시\n                this.sido_sgg_value = this.sido_sgg + \" \" + this.sido_sgg     \n                }\n            if(this.heart == true){\n                console.log(\"heart: \",this.heart)\n                console.log(\"id\",this.user.id)\n                console.log('sido_sigg',this.sido_sgg_value )\n                this.apiAddStored()\n            }else{\n                //삭제\n                this.apiDeleteStored()   \n            }\n            \n        },\n        async apiDeleteStored(){\n            try{\n            const cities = await axios.put(\n                `${BASE_URL}/user/deleteStoredCity`,\n                {\n                    id : this.user.id,\n                    sido_sgg: this.sido_sgg_value\n                })\n                console.log(\"deletecities:\", cities.data)\n                this.updateUserStoredCities(cities.data)\n                console.log(\"after delete: \",this.userStoredCities)\n            }catch(err){\n            console.log(err)\n            }\n        },\n        async apiAddStored(){\n            try{\n            const cities = await axios.put(\n                `${BASE_URL}/user/addStoredCity`,\n                {\n                    id : this.user.id,\n                    sido_sgg: this.sido_sgg_value\n                })\n                console.log(\"cities:\", cities.data)\n                this.updateUserStoredCities(cities.data)\n                console.log(\"after add: \", this.userStoredCities)\n            }catch(err){\n            console.log(err)\n            }\n        },\n        gotoDetailPage () {\n            let routeData = this.$router.resolve({name: 'details', query: {name: this.checkSido_sgg}});\n            window.open(routeData.href, '_blank');\n        },\n        \n    }\n}\n</script>"],"sourceRoot":"src/components"}]}